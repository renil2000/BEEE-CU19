void setup() {
  pinMode(8,OUTPUT);
  pinMode(9,OUTPUT);
  pinMode(10,OUTPUT);
  pinMode(11,OUTPUT);
  pinMode(12,OUTPUT);
  pinMode(13,OUTPUT);
  

}

void loop() {
  digitalWrite(8,HIGH);
  digitalWrite(8,LOW);
  delay(100);
  digitalWrite(9,HIGH);
  digitalWrite(9,LOW);
  delay(100);
  digitalWrite(10,HIGH);
  digitalWrite(10,LOW);
  delay(100);
  digitalWrite(11,HIGH);
  digitalWrite(11,LOW);
  delay(100);
  digitalWrite(12,HIGH);
  digitalWrite(12,LOW);
  delay(100);
  digitalWrite(13,HIGH);
  digitalWrite(13,LOW);
  delay(100);
}